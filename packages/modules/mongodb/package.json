{
  "name": "@naksilaclina/mongodb",
  "version": "1.0.0",
  "private": true,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "start": "node dist/index.js",
    "lint": "eslint src bin",
    "seed": "cross-env NODE_ENV=development ts-node src/seed/index.ts",
    "seed:force": "cross-env NODE_ENV=development ts-node src/seed/index.ts --force",
    "seed:clear": "cross-env NODE_ENV=development ts-node src/seed/index.ts --clear",
    "migrate-passwords": "cross-env NODE_ENV=development ts-node src/migrations/update-user-passwords.ts"
  },
  "dependencies": {
    "mongoose": "^6.8.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "bcrypt": "^5.1.0",
    "@types/bcrypt": "^5.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "mongoose": "^6.8.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}